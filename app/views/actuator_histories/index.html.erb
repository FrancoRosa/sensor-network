
<h1>Actuator Histories</h1>

<table>
  <thead>
    <tr>
      <th>Actuator</th>
      <th>Expected status</th>
      <th>Current status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actuator_histories.each do |actuator_history| %>
      <tr>
        <td><%= actuator_history.actuator_id %></td>
        <td><%= actuator_history.expected_status %></td>
        <td><%= actuator_history.current_status %></td>
        <td><%= link_to 'Show', actuator_history %></td>
        <td><%= link_to 'Edit', edit_actuator_history_path(actuator_history) %></td>
        <td><%= link_to 'Destroy', actuator_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actuator History', new_actuator_history_path %>
