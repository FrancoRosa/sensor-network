<p id="notice"><%= notice %></p>

<h1>Sensor Histories</h1>

<table>
  <thead>
    <tr>
      <th>Sensor</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sensor_histories.each do |sensor_history| %>
      <tr>
        <td><%= sensor_history.sensor_id %></td>
        <td><%= sensor_history.value %></td>
        <td><%= link_to 'Show', sensor_history %></td>
        <td><%= link_to 'Edit', edit_sensor_history_path(sensor_history) %></td>
        <td><%= link_to 'Destroy', sensor_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sensor History', new_sensor_history_path %>
